<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>E-Colimmerce</title>
    <link rel="stylesheet" href="/css/style.css" />
  </head>
  <body>
    <header>
      <nav class="nav">
        <a href="/"><h1>E-Colimmerce</h1></a>
      </nav>
    </header>

    <main>
      <section class="main__section">
        <% list.forEach((e) => { %>
        <a href="/api/carrito/<%= e.id %>/productos"
          ><div><h2 class="main__title">Carrito Nro <%= e.id %></h2></div></a
        >
        <% }); %>
      </section>
    </main>
  </body>
</html>
